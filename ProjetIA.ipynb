    "df_two = df_two.drop([\"No\"], axis=1)\n",
    " \n",
    "# Rename the Column\n",
    "result = df_two.rename(columns={\"Yes\": \"Attrition\"})\n",
    "\n",
    "dataset_test = dataset.copy()\n",
    "dataset_test[\"Attrition\"] = result[\"Attrition\"]\n",
    "\n",
    "dataset_test"
